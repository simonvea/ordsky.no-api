{
  "name": "ordsky-api",
  "description": "The backend API for ordsky.no",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "@types/aws-lambda": "^8.10.64",
    "@types/node": "^14.14.10",
    "aws-sdk": "^2.437.0",
    "aws-xray-sdk": "^3.2.0",
    "jest": "^24.7.1",
    "opt": "file:./src/common",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc",
    "tsc": "tsc",
    "check": "tsc --noEmit",
    "install:all": "cd src && for D in */; do npm i; done"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jestEnv.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/.aws-sam/"
    ]
  }
}
